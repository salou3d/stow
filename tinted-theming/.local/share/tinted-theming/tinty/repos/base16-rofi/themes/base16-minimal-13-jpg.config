! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #3b3f2e, #3b3f2e, #242918

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #3b3f2e, #999988, #3b3f2e, #3b3f2e, #c9c6b5
rofi.color-active:  #3b3f2e, #2f1294, #3b3f2e, #3b3f2e, #2f1294
rofi.color-urgent:  #3b3f2e, #8d955a, #3b3f2e, #3b3f2e, #8d955a

! Set the desired separator style
rofi.separator-style: solid
