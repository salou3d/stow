! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #3e4061, #3e4061, #21234a

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #3e4061, #b5b5bd, #3e4061, #3e4061, #f1f0ec
rofi.color-active:  #3e4061, #002bd0, #3e4061, #3e4061, #002bd0
rofi.color-urgent:  #3e4061, #b5370c, #3e4061, #3e4061, #b5370c

! Set the desired separator style
rofi.separator-style: solid
